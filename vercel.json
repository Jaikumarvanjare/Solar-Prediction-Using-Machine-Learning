{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": { "pythonVersion": "3.10" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/predict",
            "dest": "/api/index.py"
        },
        {
            "src": "/",
            "dest": "/templates/index.html"
        }
    ]
}

